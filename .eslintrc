{
  "extends": "airbnb",
  "plugins": ["react"],
  "parser": "babel-eslint",
  "env": {
    "browser": true
    //"mocha": true
  },
  "rules": {
    "quotes": 0,
    "no-console": 1,
    "no-debugger": 1,
    "linebreak-style": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}